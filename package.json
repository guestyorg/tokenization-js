{
  "name": "@guestyci/tokenization-js",
  "version": "0.1.0",
  "description": "Guesty Tokenization SDK loading utility and types",
  "files": [
    "src/index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "jest --coverage --color",
    "bootstrapci": "ebisu bootstrap-package",
    "deployci": "ebisu bump",
    "build": "node ./build-scripts/copy-files.js",
    "prettier": "prettier **/*.{js|ts}"
  },
  "devDependencies": {
    "@guestyci/ebisu": "^3.3.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^4.2.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
