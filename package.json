{
  "name": "@guestyorg/tokenization-js",
  "version": "0.1.0",
  "description": "Guesty Tokenization SDK loading utility and types",
  "main": "index.js",
  "module": "lib/esm/guesty-tokenization-js.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "test": "jest --coverage --color",
    "clean": "rm -rf ./lib",
    "build": "yarn clean && rollup --config --bundleConfigAsCjs",
    "lint": "eslint . --fix",
    "prettier": "prettier **/*.{js|ts}",
    "release": "np --no-publish"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^4.2.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^10.0.7",
    "np": "^8.0.4",
    "prettier": "^2.8.0",
    "rollup": "^3.28.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestyorg/tokenization-js.git"
  },
  "keywords": [],
  "author": "Guesty Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guestyorg/tokenization-js/issues"
  },
  "homepage": "https://github.com/guestyorg/tokenization-js/wiki",
  "private": false
}
